{
    "name": "rappasoft/laravel-boilerplate",
    "type": "project",
    "description": "The Laravel Boilerplate Project.",
    "keywords": [
        "framework",
        "laravel",
        "boilerplate"
    ],
    "license": "MIT",
    "repositories": [
        {
            "type": "vcs",
            "url":  "git@github.com:khards/laravel-google-distance.git"
        },
        {
            "type": "vcs",
            "url":  "git@github.com:khards/Laraguard.git"
        },
        {
            "type": "vcs",
            "url":  "git@github.com:khards/framework.git"
        },
        {
            "type": "vcs",
            "url": "git@github.com:khards/laravel-password-exposed-validation-rule.git"
        },
        {
            "type": "vcs",
            "url": "git@github.com:khards/password_exposed.git"
        },
        {
            "type": "vcs",
            "url": "git@github.com:khards/psr-18-guzzle-adapter.git"
        }
    ],
    "require": {
        "php": "^8.1",
        "arcanedev/log-viewer": "8.x",
        "barryvdh/laravel-dompdf": "^2.0",
        "darkghosthunter/laraguard": "dev-master",
        "divineomega/laravel-password-exposed-validation-rule": "dev-master as v2.3.0",
        "divineomega/password_exposed": "dev-master as v3.0.1",
        "divineomega/psr-18-guzzle-adapter": "dev-master as v1.1.0",
        "fideloper/proxy": "^4.4",
        "fruitcake/laravel-cors": "^2.0",
        "guzzlehttp/guzzle": "^7.0.1",
        "jamesmills/laravel-timezone": "^1.9",
        "johngrogg/ics-parser": "^2.1",
        "lab404/laravel-impersonate": "^1.6",
        "langleyfoxall/laravel-nist-password-rules": "^v4.3.0",
        "laravel/framework": "^8.12",
        "laravel/legacy-factories": "^1.1",
        "laravel/passport": "^10.x-dev",
        "laravel/socialite": "^5.0",
        "laravel/tinker": "^2.5",
        "laravel/ui": "^3.0",
        "livewire/livewire": "^2.0",
        "pnlinh/laravel-google-distance": "dev-response-with-time",
        "rappasoft/laravel-livewire-tables": "^2.0",
        "rappasoft/lockout": "^3.0",
        "rlanvin/php-rrule": "^2.2",
        "simshaun/recurr": "^4.0",
        "spatie/icalendar-generator": "^2.5",
        "spatie/laravel-activitylog": "^3.14",
        "spatie/laravel-database-mail-templates": "^3.5",
        "spatie/laravel-permission": "^3.11",
        "spatie/laravel-schemaless-attributes": "^1.8.3",
        "srmklive/paypal": "~3.0",
        "symfony/intl": "^6.0",
        "tabuna/breadcrumbs": "^2.2",
        "unisharp/laravel-filemanager": "^v2.5.1",
        "vanilo/framework": "dev-master"
    },
    "require-dev": {

        "barryvdh/laravel-debugbar": "^3.2",
        "barryvdh/laravel-ide-helper": "^2.6",
        "codedungeon/phpunit-result-printer": "^0.30",
        "facade/ignition": "^2.5",
        "friendsofphp/php-cs-fixer": "^2.16",
        "fakerphp/faker": "^1.9.1",
        "laravel/sail": "^0.0.5",
        "mockery/mockery": "^1.4.2",
        "nunomaduro/collision": "^5.0",
        "phpunit/phpunit": "^9.3.3"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "treeware/plant": true
        }
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ],
        "post-update-cmd": [
            "@php artisan ide-helper:generate",
            "@php artisan ide-helper:meta"
        ],
        "clear-all": [
            "@php artisan clear-compiled",
            "@php artisan cache:clear",
            "@php artisan route:clear",
            "@php artisan view:clear",
            "@php artisan config:clear",
            "@php artisan permission:cache-reset",
            "composer dumpautoload -o"
        ],
        "cache-all": [
            "@php artisan config:cache",
            "@php artisan route:cache"
        ],
        "reset": [
            "composer clear-all",
            "composer cache-all"
        ],
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage",
        "format": "vendor/bin/php-cs-fixer fix --allow-risky=yes"
    }
}
